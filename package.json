{
  "name": "redux-rain",
  "version": "1.0.5",
  "description": "react and redux-observable framework",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [],
  "scripts": {
    "build": "bili",
    "lint": "eslint src/** --fix",
    "test": "nyc ava",
    "commit": "git-cz",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -r 0",
    "release": "release-it --no-git.requireCleanWorkingDir",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "url": "https://github.com/zero1five/rain.git",
    "type": "git"
  },
  "author": "zero1five<zerodengyin@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "invariant": "^2.2.4",
    "lodash": "^4.17.15",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-observable": "^1.1.0",
    "rxjs": "^6.5.2",
    "util": "^0.12.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.5.5",
    "@commitlint/read": "^7.1.2",
    "ava": "^2.2.0",
    "babel-eslint": "^10.0.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^9.0.0",
    "bili": "^4.8.1",
    "chalk": "^2.4.1",
    "commitizen": "^3.0.5",
    "conventional-changelog-cli": "^2.0.11",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "^5.11.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-node-core": "^1.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^1.3.1",
    "jsdom": "^15.1.1",
    "lint-staged": "^8.1.0",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3",
    "release-it": "^9.4.3"
  },
  "lint-staged": {
    "*.{js}": [
      "npm run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "commit-msg": "node ./scripts/commit-lint.js"
    }
  },
  "ava": {
    "require": [
      "@babel/register",
      "@babel/polyfill"
    ],
    "files": [
      "**/test/**/*.test.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
